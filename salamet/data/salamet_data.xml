        <!-- =================== SEQUENCES =================== -->

        <!-- Sequence pour les grossesses -->
        <record id="seq_salamet_grossesse" model="ir.sequence">
            <field name="name">Grossesse SALAMET</field>
            <field name="code">salamet.grossesse</field>
            <field name="prefix">GRS</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence pour les accouchements -->
        <record id="seq_salamet_accouchement" model="ir.sequence">
            <field name="name">Accouchement SALAMET</field>
            <field name="code">salamet.accouchement</field>
            <field name="prefix">ACC</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence pour les bilans prenataux -->
        <record id="seq_salamet_bilan_prenatal" model="ir.sequence">
            <field name="name">Bilan Prenatal SALAMET</field>
            <field name="code">salamet.bilan.prenatal</field>
            <field name="prefix">BPN</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence pour les consultations -->
        <record id="seq_salamet_consultation" model="ir.sequence">
            <field name="name">Consultation SALAMET</field>
            <field name="code">salamet.consultation</field>
            <field name="prefix">CONS</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence pour les patientes -->
        <record id="seq_salamet_patiente" model="ir.sequence">
            <field name="name">Patiente SALAMET</field>
            <field name="code">salamet.patiente</field>
            <field name="prefix">PAT</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence pour les médecins -->
        <record id="seq_salamet_medecin" model="ir.sequence">
            <field name="name">Médecin SALAMET</field>
            <field name="code">salamet.medecin</field>
            <field name="prefix">MED</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

